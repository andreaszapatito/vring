, 'post02000size05.dat' u 5:10  w p pt 7 ps 0.1 lc '#a2142f',x w l lw 1 lc "black"
set output "filt02000size06.tex"
p 'post02000size06.dat' u 5:7   w p pt 7 ps 0.1 lc '#4dbeee'\
, 'post02000size06.dat' u 5:8   w p pt 7 ps 0.1 lc '#77ac30'\
, 'post02000size06.dat' u 5:9   w p pt 7 ps 0.1 lc '#7e2f8e'\
, 'post02000size06.dat' u 5:10  w p pt 7 ps 0.1 lc '#a2142f',x w l lw 1 lc "black"
set output "filt02000size07.tex"
p 'post02000size07.dat' u 5:7   w p pt 7 ps 0.1 lc '#4dbeee'\
, 'post02000size07.dat' u 5:8   w p pt 7 ps 0.1 lc '#77ac30'\
, 'post02000size07.dat' u 5:9   w p pt 7 ps 0.1 lc '#7e2f8e'\
, 'post02000size07.dat' u 5:10  w p pt 7 ps 0.1 lc '#a2142f',x w l lw 1 lc "black"
