, 'post02000size04.dat' u 5:10  w p pt 7 ps 0.1 lc '#a2142f',x w l lw 1 lc "black"
set output "filt02000size05.tex"
p 'post02000size05.dat' u 5:7   w p pt 7 ps 0.1 lc '#4dbeee'\
